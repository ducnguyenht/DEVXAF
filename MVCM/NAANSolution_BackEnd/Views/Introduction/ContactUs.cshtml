
@{
    ViewBag.Title = "Contact Us";
    Layout = "~/Views/Shared/MasterPages/_Layout.cshtml";
    <script src="~/Content/ckeditor/ckeditor.js"></script>
    <script src="~/Content/ckfinder/ckfinder.js"></script>
}
<section id="main-content">
    <section class="wrapper site-min-height">
        <div class="nas-content">
            <div class="col-md-12 mt">
                <div class="col-md-12 col-xs-12 nas-header">
                    <div class="col-md-12">
                        <h1 style="font-size: 20px; margin-top: 5px;">Liên hệ</h1>
                        <hr />
                    </div>
                    <div class="panel-body">
                        <ul class="nav nav-tabs">
                            <li class="active">
                                <a href="#vn" data-toggle="tab">
                                    Tiếng Việt
                                </a>
                            </li>
                            <li>
                                <a href="#en" data-toggle="tab">
                                    Tiếng Anh
                                </a>
                            </li>
                        </ul>
                        <div class="tab-content">
                            <div class="tab-pane fade in active" id="vn">
                                <div class="col-md-12 col-sm-12 col-xs-12 nas-col" style="margin:10px 0">
                                    <div class="col-md-12 col-sm-12 col-xs-12 nas-col">
                                        <div class="col-md-12 col-sm-12 col-xs-12 nas-title">
                                            <div class="col-md-1 col-sm-3 col-xs-4 nas-col nas-title-content">
                                                Địa chỉ:
                                            </div>
                                            <div class="col-md-11 col-sm-9 col-xs-8 nas-col">
                                                <input style="width:100%;" placeholder="Nhập địa chỉ..." />
                                            </div>
                                        </div>
                                        <div class="col-md-12 col-sm-12 col-xs-12 nas-title">
                                            <div class="col-md-1 col-sm-3 col-xs-4 nas-col nas-title-content-check">
                                                Số điện thoại:
                                            </div>
                                            <div class="col-md-11 col-sm-9 col-xs-8 nas-col">
                                                <input style="width:100%;" placeholder="Nhập số điện thoại..." />
                                            </div>
                                        </div>
                                        <div class="col-md-12 col-sm-12 col-xs-12 nas-title">
                                            <div class="col-md-1 col-sm-3 col-xs-4 nas-col nas-title-content-check">
                                                Email:
                                            </div>
                                            <div class="col-md-11 col-sm-9 col-xs-8 nas-col">
                                                <input style="width:100%;" placeholder="Nhập email..." />
                                            </div>
                                        </div>
                                        <div class="col-md-12 col-sm-12 col-xs-12 nas-title">
                                            <div class="col-md-1 col-sm-3 col-xs-4 nas-col nas-title-content-check">
                                                Website:
                                            </div>
                                            <div class="col-md-11 col-sm-9 col-xs-8 nas-col">
                                                <input style="width:100%;" placeholder="Nhập website..." />
                                            </div>
                                        </div>
                                        <div class="col-md-12 col-sm-12 col-xs-12 nas-title">
                                            <div class="col-md-1 col-sm-3 col-xs-4 nas-col nas-title-content-check">
                                                Bản đồ:
                                            </div>
                                            <div class="col-md-11 col-sm-9 col-xs-8 nas-col">
                                                <input style="width:100%;" placeholder="Nhập địa chỉ bản đồ..." />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-12 nas-button">
                                        <button type="button" class="btn btn-primary">
                                            Lưu
                                        </button>
                                        <button type="button" class="btn btn-warning">
                                            Huỷ bỏ
                                        </button>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane fade" id="en">
                                <div class="col-md-12 col-sm-12 col-xs-12 nas-col" style="margin:10px 0">
                                    <div class="col-md-12 col-sm-12 col-xs-12 nas-col">
                                        <div class="col-md-12 col-sm-12 col-xs-12 nas-title">
                                            <div class="col-md-1 col-sm-3 col-xs-4 nas-col nas-title-content">
                                                Address:
                                            </div>
                                            <div class="col-md-11 col-sm-9 col-xs-8 nas-col">
                                                <input style="width:100%;" placeholder="Enter address..." />
                                            </div>
                                        </div>
                                        <div class="col-md-12 col-sm-12 col-xs-12 nas-title">
                                            <div class="col-md-1 col-sm-3 col-xs-4 nas-col nas-title-content-check">
                                                Telephone:
                                            </div>
                                            <div class="col-md-11 col-sm-9 col-xs-8 nas-col">
                                                <input style="width:100%;" placeholder="Enter telephone..." />
                                            </div>
                                        </div>
                                        <div class="col-md-12 col-sm-12 col-xs-12 nas-title">
                                            <div class="col-md-1 col-sm-3 col-xs-4 nas-col nas-title-content-check">
                                                Email:
                                            </div>
                                            <div class="col-md-11 col-sm-9 col-xs-8 nas-col">
                                                <input style="width:100%;" placeholder="Enter email..." />
                                            </div>
                                        </div>
                                        <div class="col-md-12 col-sm-12 col-xs-12 nas-title">
                                            <div class="col-md-1 col-sm-3 col-xs-4 nas-col nas-title-content-check">
                                                Website:
                                            </div>
                                            <div class="col-md-11 col-sm-9 col-xs-8 nas-col">
                                                <input style="width:100%;" placeholder="Enter website..." />
                                            </div>
                                        </div>
                                        <div class="col-md-12 col-sm-12 col-xs-12 nas-title">
                                            <div class="col-md-1 col-sm-3 col-xs-4 nas-col nas-title-content-check">
                                                Map:
                                            </div>
                                            <div class="col-md-11 col-sm-9 col-xs-8 nas-col">
                                                <input style="width:100%;" placeholder="Enter url map..." />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-12 nas-button">
                                        <button type="button" class="btn btn-primary">
                                            Save
                                        </button>
                                        <button type="button" class="btn btn-warning">
                                            Cancel
                                        </button>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>

                </div>
            </div>
        </div>
    </section>
</section>

